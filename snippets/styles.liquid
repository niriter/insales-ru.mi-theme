{% if settings.font_family_primary contains 'google:' %}
  {% assign font_family_primary = settings.font_family_primary | replace_first: 'google:', '' %}
  <link href="//fonts.googleapis.com/css?family={{ font_family_primary }}:300,400,700&subset=cyrillic,latin" rel="stylesheet" />
{% endif %}

{% if settings.logotype_type == 'text' %}
  {% if settings.logotype_font_family contains 'google:' and settings.font_family_primary != settings.logotype_font_family %}
    {% assign logotype_font_family = settings.logotype_font_family | replace_first: 'google:', '' %}
    <link href="//fonts.googleapis.com/css?family={{ logotype_font_family }}:300,400,700&subset=cyrillic,latin" rel="stylesheet" />
  {% endif %}
{% endif %}
<link href="{{ 'theme.css' | asset_url }}" rel="stylesheet" type="text/css" />
<script src="{{ 'icons.js' | asset_url }}" charset="utf-8"></script>
<script src="{{ 'theme.js' | asset_url }}" charset="utf-8"></script> 
