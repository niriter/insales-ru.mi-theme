  <script>
    (function(retailrocket) {
      'use strict';

      function waitFor(exitCondition, callback) {
        var checkCount = 10;
        var timeoutms = 1000;

        (function check() {
          if (exitCondition()) {
            callback();
            return;
          }

          if (checkCount === 0) {
            return;
          }

          checkCount -= 1;
          setTimeout(check, timeoutms);
        }());
      }

      retailrocket.ruMiExitIntent = (function() {
        return {
          rrSubmit: function() {
            var inputElem = document.getElementById('retailrcoket-mailrequest-form-email');
            var emailValue = inputElem.value;
            var errorClass = 'rr-error';

            if (!retailrocket.modules.utils.isValidEmail(emailValue)) {
              inputElem.className += ' ' + errorClass;
              return;
            }

            inputElem.className = inputElem.className.replace(new RegExp('\\b' + errorClass + '\\b', 'g'), '');

            var currentDateTime = new Date();
            var hourOfMillisecs = 3600000;
            var startTimeOfCurrentYear = (new Date(currentDateTime.getFullYear(), 0, 1)).getTime();
            var currentTime = currentDateTime.getTime();
            var pastTimeOfStartCurrentYear = currentTime - startTimeOfCurrentYear;
            var hoursOfOneWeek = 168;
            var week = Math.ceil(pastTimeOfStartCurrentYear / hourOfMillisecs / hoursOfOneWeek);

            rrApi.setEmail(emailValue, {
              Week: week,
              formId: 'coupon',
              promo: 1
            });
            rrApi.welcomeSequence(emailValue);
            waitFor(
              function() {
                return ('ga' in window);
              },
              function() {
                window.ga('send', {
                  hitType: 'event',
                  eventCategory: 'Subscribe',
                  eventAction: 'send',
                  eventLabel: 'RR_pop-up'
                });
              }
            );
            document.getElementById('rr-popup-content').style.display = 'none';
            document.getElementById('rr-popup-message').style.display = 'block';

            setTimeout(function() {
              retailrocket.exitIntendWidget.close();
              document.getElementById('rr-popup-message').style.display = 'none';
            }, 2500);

            retailrocket.exitIntendWidget.close();
          },
          closeMessage: function() {
            document.getElementById('rr-popup-message').style.display = 'none';
          },
        };
      }());

      waitFor(
        function() {
          return ('ga' in window);
        },
        function() {
          window.ga('ec:addPromo', {
            id: 'RR_email_popup',
            name: 'RR_email_popup_v2 ',
          });
        }
      );

      (function() {
        var exitIntentContainer = document.querySelector('.rumi-exit-intend2');
        var isClientFromYandexMarket = (function() {
          var yamarketRX = /\butm_source=(?:yandexmarket|yamarket)\b/i;

          return yamarketRX.test(window.location.search) || yamarketRX.test(document.cookie);
        }());

        if (!isClientFromYandexMarket) {
          retailrocket.exitIntendWidget.initialize();
          retailrocket.mailRequestForm.initialize();
          retailrocket.deactivateIfAgreedToReceiveMarketingMail.initialize();
          retailrocket.widget.render();
        } else {
          exitIntentContainer.classList.add('_disable');
        }
      }());
    }(retailrocket));
