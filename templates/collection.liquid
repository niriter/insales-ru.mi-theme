{% include 'breadcrumb' %}

<h1 itemprop="name">{{collection.title}}</h1>




<div id="column-left" class="">
  <div class="box-cat">
    <div class="box-cat-heading">Категории товаров</div>
    <div class="box-cat-content">
      <ul class="box-cat-category">
        {%for collection in collection.subcollections%}
          <li>
            <a href="{{collection.url}}">{{collection.title}}</a>
            {% assign subcoll = collection.subcollections | size%}
            {%if subcoll > 0%}
              <div class="category_open_also"></div>
              <div class="category_2nd_also">
                <ul class="box-cat-category">
                  {%for collection in collection.subcollections%}
                    <li>
                      <a href="{{collection.url}}">{{collection.title}}</a>
                    </li>
                  {%endfor%}
                </ul>
              </div>
            {%endif%}
          </li>
        {%endfor%}
      </ul>
    </div>
  </div>
  <script type="text/javascript">
    $(document).ready(function() {
      $('.box-cat-heading').live('click', function() {
        if ($(this).hasClass('box-cat-heading-active')) {
          $('.box-cat-content>.box-cat-category').css('display', 'block');
          $(this).removeClass('box-cat-heading-active');
        } else {
          $('.box-cat-content>.box-cat-category').css('display', 'none');
          $(this).addClass('box-cat-heading-active');
        }
      });
    });
  </script>
  <div class="homepage_banner w100">
    <div class="product-of-day">
      {%assign product_day_count = 0%}
      {% for product in collections.all.products %}
        {%if product.properties.label.characteristics[0].name == "Товар Дня" and product_day_count < 1 %}
          {%assign product_day_count = 1%}
          <div class="product_of_day">
            <span class="economy_text">Экономия</span>
            <span class="economy_price">{{product.old_price | minus: product.price | round: 0}} <span class="rouble"> руб.</span></span>
          </div>
          <div class="day_title">Товар дня</div>
          <div class="image">
            <a href="{{product.url}}"><img src="{{product.first_image.original_url}}" alt="{{product.title}}" /></a>
          </div>
          <div class="category_name">
            {{product.collections.last.title}}
          </div>
          <div class="name"><a href="{{product.url}}">{{product.title}}</a></div>
          <div class="description"></div>
          <div class="rating">
            <div class="rating_left">
              <a class="rating_stars" href="{{product.url}}">
                <div class="rating_full" style="width: 100%"></div>
              </a>
            </div>
            <div class="rating_right">
              <a class="rating_reviews" href="{{product.url}}">{{product.reviews | size}}</a>
            </div>
          </div>
          <div class="product_bottom">
            <form action="{{ cart_url }}" method="post" data-product-id="{{ product.id }}">
              <input type="hidden" name="variant_id" value="{{product.variants.first.id}}" >
              <div style="display: none;" data-quantity>
                <input type="text" name="quantity" value="1" />
              </div>
              <div class="price">
                {%if product.old_price%}<span class="price-old">{{product.old_price | round: 0}}</span>{%endif%} <span class="price-new">{{product.price | money}}</span>
              </div>
              <div class="cart"><a class="button" data-item-add><span></span><div class="add_to_cart_help">Товар в корзине</div></a></div>
              <div class="cl_b"></div>
              </form>
            </div>
          {%break%}
        {%endif%}
      {%endfor%}
      </div>
  </div>
  <div class="team_banners">
    <a href="https://ru-mi.com/contact/" data-promo="{&quot;id&quot;:&quot;Rumicom Team&quot;,&quot;name&quot;:&quot;rumi_team_banner_sergey.jpg&quot;,&quot;creative&quot;:&quot;&quot;}" onclick="ee_promo.click(this); return !ga.loaded;">
      <img src="image/data/team_banners/rumi_team_banner_sergey.jpg">
    </a>
  </div>
  <script type="text/javascript">
    <!--
    ga('ec:addPromo', {
      'id': 'Rumicom Team',
      'name': 'rumi_team_banner_sergey.jpg',
      'creative': ''
    });
    ga('send', 'event', 'promo', 'impression', {
      'nonInteraction': true
    });

    function onPromoClick(promoObj, href) {
      ga('ec:addPromo', {
