            <label class="label-field hide">Сортировать: </label>
            <select class="select-field js-filter-trigger" name="order">
              {{ "" | select_option: order, "По умолчанию" }}
              {{ "descending_popularity" | select_option: order, "По популярности" }}
              {{ "price"            | select_option: order, "По возрастанию цене" }}
              {{ "descending_price" | select_option: order, "По убыванию цены"  }}
              {{ "descending_age"   | select_option: order, "По новинкам" }}
              {{ "descending_discount" | select_option: order, "По скидке" }}
              {{ "title" | select_option: order, "По алфавиту" }}
            </select>
