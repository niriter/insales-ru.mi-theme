    <div class="homepage_banner">
      <div class="product-of-day">
        {%assign product_day_count = 0%}
        {% for product in collections.all.products %}
          {%if product.properties.label.characteristics[0].name == "Товар Дня" and product_day_count < 1 %}
            {%assign product_day_count = 1%}
            <div class="product_of_day">
              <span class="economy_text">Экономия</span>
              <span class="economy_price">{{product.old_price | minus: product.price | round: 0}} <span class="rouble"> руб.</span></span>
            </div>
            <div class="day_title">Товар дня</div>
            <div class="image">
              <a href="{{product.url}}"><img src="{{product.first_image.original_url}}" alt="{{product.title}}" /></a>
            </div>
            <div class="category_name">
              {{product.collections.last.title}}
            </div>
            <div class="name"><a href="{{product.url}}">{{product.title}}</a></div>
            <div class="description"></div>
            <div class="rating">
              <div class="rating_left">
                <a class="rating_stars" href="{{product.url}}">
                  <div class="rating_full" style="width: 100%"></div>
                </a>
              </div>
              <div class="rating_right">
                <a class="rating_reviews" href="{{product.url}}">{{product.reviews | size}}</a>
              </div>
            </div>
            <div class="product_bottom">
              <div class="price">
                {%if product.old_price%}<span class="price-old">{{product.old_price | round: 0}}</span>{%endif%} <span class="price-new">{{product.price | money}}</span>
              </div>
              <div class="cart"><a class="button"><span></span><div class="add_to_cart_help">Товар в корзине</div></a></div>
              <div class="cl_b"></div>
            </div>
            {%break%}
          {%endif%}
        {%endfor%}
      </div>
    </div>
  </div>
  <div class="tovars">
    <div class="rr-widget cartRelated rr-active">
      <header class="widgettitle">Акции</header>
      <ul class="items">
        {% assign products_sale = 0 %}
        {% for product in collections.all.products %}
          {%if product.old_price and product.old_price > product.price %}
            {%assign products_sale = products_sale | plus: 1%}
            {% if products_sale <= 15 %}
              <li class="item">
                <div class="block">
                  <div class="rr-header">
                    <div class="rr-labels">
                      <div class="rr-discount">
                        -{{ product.old_price | minus: product.price | divided_by: product.old_price | times: 100 | round: 0}}%
                      </div>
                    </div>
                    <div class="rr-icons">
                      <a class="product_features_wishlist" product_id="9649" tabindex="-1"></a>
                      <span class="product_features_compare " product_id="9649"></span>
                    </div>
                  </div>
                  <a class="item-info" href="{{product.url}}" tabindex="-1">
                    <div class="item-image">
                      <img src="{{product.first_image.original_url}}">
                    </div>
                    <div class="item-title">{{product.title}}</div>
                  </a>
                  <div class="rating">
                    <div class="rating_left">
                      <a class="rating_stars" href="{{product.url}}" tabindex="-1">
                        {%if product.rating %}
                          <div class="rating_full stars_{{ product.rating }}"></div>
                        {%else%}
                          <div class="rating_full stars_0"></div>
                        {%endif%}
                      </a>
                    </div>
                    <div class="rating_right">
                      <a class="rating_reviews" href="{{product.url}}" tabindex="-1">{{product.reviews | size}}</a>
                    </div>
                  </div>
                  <div class="item-price-and-buy">
                    <div class="item-block-price">
                      <div class="item-old-price">
                        <span class="item-old-price-value">{{product.old_price | round: 0}}</span>
                        <span class="item-price-currency"></span>
                      </div>
                      <div class="item-price">
                        <span class="item-price-value">{{product.price | money}}</span>
                      </div>
                    </div>
                    <div class="item-actions">
                      <a class="item-actions-buy add_to_cart_9649" tabindex="-1">
                        <div class="add_to_cart_help">Товар в корзине</div>
                      </a>
                    </div>
                  </div>
                </div>
              </li>
            {%else%}
              {%break%}
            {%endif%}
          {%else%}
            {%continue%}
          {%endif%}
        {%endfor%}
      </ul>
    </div>

